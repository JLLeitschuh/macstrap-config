#!/bin/sh

# Make vim the default editor.
export EDITOR='vim';

# Prefer US English and use UTF-8.
export LANG='en_US.UTF-8';
export LC_ALL='en_US.UTF-8';

# Highlight section titles in manual pages.
export LESS_TERMCAP_md="${yellow}";

# Donâ€™t clear the screen after quitting a manual page.
export MANPAGER='less -X';

# Export JDK environment variable used for build
export JDK7=$(asdf where java zulu-7.48.0.11)
export JDK8=$(asdf where java temurin-8.0.302+8)
export JDK11=$(asdf where java temurin-11.0.12+7)
export JDK16=$(asdf where java temurin-16.0.2+7)
